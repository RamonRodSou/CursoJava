package fundamentos;

public class CalcularATemperatura {
	public static void main(String[] args) {
	
		// ( F - 32 ) * 5 / 9  = C
		
		final double TRINTAEDOIS = 32;
		final double CINDODIVNOVE = 0.55;
		
		double fahrenheit = 120.3;
		
		double celsius = ( fahrenheit - TRINTAEDOIS ) * CINDODIVNOVE;
		
		System.out.println(fahrenheit + " fahrenheit para celsius Ã© igual a: " + celsius);
		
	}

}
